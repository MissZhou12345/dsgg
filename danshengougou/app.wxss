@import "dist/index.wxss";

.container {
    position: relative;
    overflow: hidden;
    min-height: 100vh;
    box-sizing: border-box;
    padding: 0;
    color: #777;
    font-size: 12px;
}

.container::before {
    position: fixed;
    top: 0;
    left: 0;
    content: ' ';
    width: 100%;
    z-index: 5;
}

.doc-title {
    position: relative;
    padding: 15px 0;
    margin: 10px 15px;
    line-height: 25px;
    font-size: 25px;
    color: #666;
}

.doc-description {
    margin: 14px 0;
    padding: 0 15px;
    font-size: 14px;
    line-height: 20px;
    color: #666;
}


